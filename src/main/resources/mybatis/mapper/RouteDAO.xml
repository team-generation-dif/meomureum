<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.meomureum.springboot.dao.IRouteDAO">
	<insert id="insertDAO">
		insert into route
		 values('R'||lpad(schedule_seq.nextval,6,'0'),#{p_code,jdbcType=VARCHAR},#{s_code},#{r_order},#{r_memo},#{r_day})
	</insert>
	<delete id="deleteDAO">
		delete from route where s_code = #{s_code}
	</delete>
</mapper>